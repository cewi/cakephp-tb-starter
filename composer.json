{
    "name": "cewi/cakephp_tb_starter",
    "description": "Cakephp 3 plugin which extends FriendsOfCake/bootstrap-ui with some added functionallity",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp", 
        "twitter", 
        "bootstrap"
    ], 
    "homepage": "http://github.com/cewi/cakephp_tb_starter", 
    "license": "MIT", 
    "authors": [
        {
            "name": "Cornelius Wichmann",
            "role": "Author"
        }
    ], 
    "support": {
        "issues": "http://github.com/cewi/cakephp_tb_starter/issues", 
        "source": "http://github.com/cewi/cakephp_tb_starter"
    },
    "require": {
        "php": ">=5.4",
        "cakephp/plugin-installer": "*",
        "cakephp/cakephp": "~3.0",
        "cakedc/search": "3.0.*-dev",
        "friendsofcake/bootstrap-ui": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Cewi\\CakephpTbStarter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cewi/CakephpTbStarter\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
